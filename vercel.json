{
  "version": 2,
  "buildCommand": "",
  "framework": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/html/index.html"
    },
    {
      "source": "/((?!src|assets).*)",
      "destination": "/src/html/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/src/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/src/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
